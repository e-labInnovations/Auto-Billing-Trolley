        "z": "7c45b39da721d99e",
        "name": "addItem",
        "func": "const receivedData = JSON.parse(msg.payload)\nconst products = global.get(\"products\")\nconst cart = global.get(\"cart\") || []\n\nif (receivedData.command == \"addItem\") {\n    msg.topic = \"cartItems\"\n    let item = products.find(_item => _item.rfid == receivedData.rfid)\n    let isAlreadyAdded = item && cart.find(_item => _item.rfid == receivedData.rfid)\n    if (item && !isAlreadyAdded)\n        cart.push(item)\n    \n    global.set(\"cart\", cart)\n    msg.payload = cart\n    return msg;\n}",
